[project]
	description = Kodi with a CMake based build system. Respect the GPL and do whatever you want with this code.
[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/girisandeep
[access "refs/for/refs/*"]
	read = group user/girisandeep
	push = group user/girisandeep
	label-Code-Review = -2..+2 group user/girisandeep
	label-Verified = -1..+1 group user/girisandeep
	rebase = group user/girisandeep
	removeReviewer = group user/girisandeep
	submit = group user/girisandeep
[access "refs/heads/*"]
	read = group user/girisandeep
	create = group user/girisandeep
	push = +force group user/girisandeep
	pushMerge = group user/girisandeep
[access "refs/tags/*"]
	push = +force group user/girisandeep
	pushTag = group user/girisandeep
	pushSignedTag = group user/girisandeep
